#include<stdio.h>
int main()
{
	printf("Samuvel Raj\n192321159\n");
	int a[100][100],n;
	int sum=0;
	printf("Enter array Size\n");
	scanf("%d",&n);
	printf("Enter array A element\n");
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<n;j++){
		
		scanf("%d",&a[i][j]);
		if(i==j)
		{
			sum=sum+a[i][j];
		}
	}
}
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<n;j++){
		
		printf("%d ",a[i][j]);
		
	}
	printf("\n");
}
printf("sum fo diagonal is %d",sum);
}
